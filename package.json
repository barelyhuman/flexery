{
  "name": "flexery",
  "version": "0.0.9",
  "description": "Flex modifier utilities for working with React",
  "keywords": [
    "react",
    "component",
    "components",
    "util",
    "utils",
    "flex",
    "flex-utils"
  ],
  "repository": "barelyhuman/flexery",
  "license": "MIT",
  "author": {
    "name": "Reaper",
    "email": "ahoy@barelyhuman.dev",
    "url": "https://barelyhuman.dev"
  },
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./index.js",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "packageManager": "pnpm@8.9.0",
  "scripts": {
    "build": "rimraf dist; pnpm build:modules; pnpm copy",
    "postbuild": "pnpm copy",
    "build:modules": "tsup src/index.js --loader '.js=jsx' --format cjs,esm --clean --dts --target=node12.22",
    "copy": "copyfiles -f package.json readme.md license dist && json -I -f dist/package.json -e \"this.private=false; this.devDependencies=undefined; this.optionalDependencies=undefined; this.scripts=undefined;this.sizesnap=undefined; this.prettier=undefined; this.jest=undefined; this['lint-staged']=undefined;\"",
    "dev": "pnpm run build -- --watch",
    "next": "bumpp --commit --tag",
    "fix": "prettier --write .",
    "size": "sizesnap",
    "test": "ava",
    "test:ci": "c8 ava"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "prettier": "@barelyhuman/prettier-config",
  "ava": {
    "require": [
      "@babel/register"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@babel/register": "^7.22.15",
    "@barelyhuman/prettier-config": "^1.1.0",
    "@types/react": "^18.2.27",
    "ava": "^5.3.1",
    "bumpp": "^9.2.0",
    "c8": "^8.0.1",
    "copyfiles": "^2.4.1",
    "json": "^11.0.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "sizesnap": "^0.2.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": ">=16.7"
  },
  "engines": {
    "node": ">=12"
  },
  "sizesnap": {
    "files": [
      "./dist/**/*.cjs",
      "./dist/**/*.mjs",
      "./dist/**/*.ts"
    ]
  }
}
